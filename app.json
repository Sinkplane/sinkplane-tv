{
  "expo": {
    "scheme": "routertv",
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/tv_icons/icon-400x240.png",
          "appleTVImages": {
            "icon": "./assets/tv_icons/icon-1280x768.png",
            "iconSmall": "./assets/tv_icons/icon-400x240.png",
            "iconSmall2x": "./assets/tv_icons/icon-800x480.png",
            "topShelf": "./assets/tv_icons/icon-1920x720.png",
            "topShelf2x": "./assets/tv_icons/icon-3840x1440.png",
            "topShelfWide": "./assets/tv_icons/icon-2320x720.png",
            "topShelfWide2x": "./assets/tv_icons/icon-4640x1440.png"
          }
        }
      ],
      "expo-router",
      "expo-font",
      "expo-build-properties",
      "expo-web-browser",
      "expo-secure-store",
      "react-native-video"
    ],
    "android": {
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/images/splash.png"
      },
      "package": "com.cpawl89.sinkplane",
      "permissions": [
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CHANGE_WIFI_MULTICAST_STATE",
        "android.permission.INTERNET"
      ]
    },
    "ios": {
      "splash": {
        "image": "./assets/images/splash.png"
      },
      "bundleIdentifier": "com.cpawl89.sinkplane",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSBonjourServices": ["sinkplane-tv._tcp"],
        "NSLocalNetworkUsageDescription": "Allow sinkplane companion app to discover tv apps on the local network."
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    },
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "name": "sinkplane",
    "slug": "sinkplane",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6233ebc0-94dc-488d-81df-0b82ee578bb8"
      }
    }
  }
}
